<h2>My rentals</h2>
<% @rentals.each do |rental| %>
  <div class="rental">
    <h3>Rental nummber:<%= rental.id%> Starship:<% rental.starship.name %></h3>
    <p><%= rental.starship.photo %></p>
    <p><%= rental.description %></p>
    <p>Start:<%= rental.start_date %> End:<%= rental.end_date %></p>
    <p><%= rental.price %></p>
  </div>
<% end %>
